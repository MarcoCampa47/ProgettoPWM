<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/constraintLayoutPaymentTickesFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".modules.ticketsModule.resultModule.TicketsResultFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/topBarContainerPayamentTicketsFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/TopBarTextViewPayamentTicketsFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Biglietti trovati"
            android:textColor="@color/black"
            android:textSize="26dp"
            android:textStyle="bold"
            android:layout_margin="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </TextView>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvFragmentTicketsPayment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        app:layout_constraintEnd_toEndOf="@+id/constraintLayoutPaymentTickesFragment"
        app:layout_constraintStart_toStartOf="@+id/constraintLayoutPaymentTickesFragment"
        app:layout_constraintTop_toBottomOf="@+id/topBarContainerPayamentTicketsFragment"
        tools:listitem="@layout/item_rv_results_tickets_fragment" />
</androidx.constraintlayout.widget.ConstraintLayout>



